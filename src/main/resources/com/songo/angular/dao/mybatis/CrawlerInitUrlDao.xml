<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.songo.angular.dao.CrawlerInitUrlDao">
	
	<select id="selects" parameterType="java.lang.Boolean" resultType="CrawlerInitUrl">
		select 
			* 
		from
			t_crawler_init_url
		where 
			visit = #{visit}
	</select>

</mapper> 